{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.common.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "analyze:dev": "webpack --config webpack.dev.js --profile --json=webpack.stats.json && webpack-bundle-analyzer ./webpack.stats.json",
    "analyze": "webpack --config webpack.prod.js --profile --json=webpack.stats.json && webpack-bundle-analyzer ./webpack.stats.json",
    "lint": "eslint --fix --format=codeframe ./",
    "format": "prettier --write --ignore-path=.eslintignore ./",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ms2052001/react-webpack.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ms2052001/react-webpack/issues"
  },
  "homepage": "https://github.com/ms2052001/react-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@hot-loader/react-dom": "^17.0.1",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "pinst": ">=2",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.1.1",
    "syncpack": "^5.7.11",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "prettier": {
    "trailingComma": "none"
  },
  "volta": {
    "node": "14.16.1",
    "yarn": "1.22.10"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
